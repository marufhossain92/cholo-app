{
  "expo": {
    "name": "cholo-app",
    "slug": "cholo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cholo-app",
      "config": {
        "googleMapsApiKey": "AIzaSyAPp_3eP4Rw8Ck6GAZbtK8asNyaJL5-qDE"
        }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.cholo-app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAPp_3eP4Rw8Ck6GAZbtK8asNyaJL5-qDE"
        },
        "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "FOREGROUND_SERVICE"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
